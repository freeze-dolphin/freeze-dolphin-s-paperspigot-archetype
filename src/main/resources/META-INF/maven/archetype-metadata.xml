<archetype-descriptor
	xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.1.0 http://maven.apache.org/xsd/archetype-descriptor-1.1.0.xsd"
	name="taboolib-quickstart-archetype">
	<requiredProperties>
		<requiredProperty key="name">
			<defaultValue>${artifactId}</defaultValue>
		</requiredProperty>
		<requiredProperty key="author">
			<defaultValue></defaultValue>
		</requiredProperty>
		<requiredProperty key="license">
			<defaultValue>GPL-3.0</defaultValue>
		</requiredProperty>
		<requiredProperty key="apiVersion">
			<defaultValue>1.13</defaultValue>
		</requiredProperty>
		<requiredProperty key="load">
			<defaultValue>POSTWORLD</defaultValue>
			<validationRegex>(POSTWORLD|STARTUP)</validationRegex>
		</requiredProperty>
		<requiredProperty key="prefix">
			<defaultValue>${name}</defaultValue>
		</requiredProperty>
	</requiredProperties>

	<fileSets>
		<fileSet encoding="UTF-8" packaged="true" filtered="true">
			<directory>src/main/java</directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
		<fileSet encoding="UTF-8" filtered="true">
			<directory>src/main/resources</directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
	</fileSets>
</archetype-descriptor>